{
    "model": {
        "backend":              "mobilenet",
        "input_size_w":         224,
        "input_size_h":         224
    },

    "train": {
        "nb_epochs":            30,
        "warmup_epochs":        0,
        "batch_size":           4,
        "learning_rate":        1e-3,
        "lamb_obj":             1.0,
        "lamb_noobj":           0.5,
        "lamb_coord":           1.0,
        "lamb_class":           1.0
    },

    "predict": {
        "iou_threshold":            0.7,
        "score_threshold":          0.5
    }

}